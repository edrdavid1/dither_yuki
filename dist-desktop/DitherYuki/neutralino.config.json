{
  "applicationId": "com.ditheryuki.app",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "enableServer": true,
  "enableNativeAPI": true,
  "singlePageServe": true,
  "documentRoot": "/dist",
  "url": "/index.html",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "dataLocation": ".dither-yuki",
  "storageLocation": "storage",
  "modes": {
    "window": {
      "title": "Dither Yuki",
      "width": 1200,
      "height": 800,
      "resizable": true,
      "center": true,
      "useSavedState": true,
      "enableInspector": true,
      "openInspectorOnStartup": true,
      "webviewArgs": "--disable-gpu",
      "icon": "./public/icon.icns",
      "injectGlobals": true,
      "injectClientLibrary": true
    }
  },
  "cli": {
    "resourcesPath": "/dist",
    "distributionPath": "/dist-desktop",
    "binaryName": "DitherYuki",
    "binaryVersion": "v6.3.0",
    "clientVersion": "v4.13.0",
    "frontendLibrary": {
      "name": "vite"
    },
    "hostProject": {
      "projectPath": "/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build",
      "buildPath": "/dist"
    },
    "copyItems": [
      "/neutralino.config.json"
    ]
  }
}