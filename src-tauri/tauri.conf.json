{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Dither Yuki",
  "version": "1.0.0",
  "identifier": "com.ditheryuki",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Dither Yuki",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/icon.ico"
    ]
  }
}
